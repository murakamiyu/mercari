<main class="l-container clearfix">
  <div class="l-content">
    <section class="mypage-tab-container">
      <h2 class="mypage-tab-head">出品した商品</h2>
      <ul class="listing-tabs">
        <li class="active">
          <h3>
            <a href="#mypage-tab-transaction-now" data-toggle="tab">
            出品中
            </a>
          </h3>
        </li>
        <li class="">
          <h3>
            <a href="/mypage/listings/in_progress/">
            取引中
            </a>
          </h3>
        </li>
        <li class="">
          <h3>
            <a href="/mypage/listings/completed/">
            売却済み
            </a>
          </h3>
        </li>
      </ul>
      <div class="tab-content">
        <ul id="mypage-tab-transaction-now" class="mypage-item-list tab-pane active">
          <li class="mypage-item-not-found bold">出品中の商品がありません</li>
        </ul>
      </div>
    </section>
    <ul class="mypage-history pager">
      <li class="pager-prev pager-cell">
        <a href="/mypage/listings/listing/"><i class="icon-arrow-left"></i></a>
      </li>
      <li class="pager-next pager-cell">
            <a href="/mypage/listings/listing/"><i class="icon-arrow-right"></i></a>
      </li>
    </ul>
  </div>
  <%= render "shared/sidebar" %>
</main>
